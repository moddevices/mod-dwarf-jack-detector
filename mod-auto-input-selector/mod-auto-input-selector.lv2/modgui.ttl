@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://mod.audio/plugins/mod-auto-input-selector>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/mod-auto-input-selector.html> ;
        modgui:stylesheet <modgui/mod-auto-input-selector.css> ;
        modgui:javascript <modgui/mod-auto-input-selector.js> ;
        modgui:screenshot <modgui/mod-auto-input-selector_ss.png> ;
        modgui:thumbnail <modgui/mod-auto-input-selector_tn.png> ;
        modgui:discussionURL <https://forum.mod.audio/t/mod-auto-input-selector-auto-output-selector/9116> ;
        modgui:brand "MOD" ;
        modgui:label "Auto Input Selector" ;
        modgui:monitoredOutputs [
            lv2:symbol "Status";
        ] ;
    ] .
